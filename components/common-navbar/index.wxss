.container{
  width: 100%;
  height: 145rpx;
  background-color: #ffffff;
  position: fixed;
  z-index: 999;
}

.inner-container {
  width: 400rpx;
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  position: relative;
  left: 20rpx;

}

.img-container{
    display:flex;
    flex-direction:column;
    justify-content: center;
    align-items: center;
}

.img {
  width: 50rpx;
  height: 50rpx;
  display: flex;
}

.title-container{
    display:flex;
    flex-direction:column;
    justify-content: center;
    align-items: center;
}

.title{
    font-size: 28rpx;
    color: #000000FF;
}

.placeholder {
  height: 145rpx;
}